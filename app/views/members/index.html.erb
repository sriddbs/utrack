<p><%= link_to 'Add new Member', new_member_path %></p>
<table>
  <thead>
    <th>Name</th>
    <th>Website</th>
    <th></th>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= link_to website_short_url(member.url_key), member_website_path(member.url_key), target: '_blank' %></td>
        <td><%= link_to 'View Profile', profile_member_path(member) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
